"""اكتب احلا و شحلا"""

# ‫استيراد مفيد.عادي مثل عادي
اكتب("اكتب احلا و شحلا")
# هذا ١ كمنت
‫عدد = ٥‬
‫لو عدد > ٢:‬
    اثنانـعدد = عدد - ٢‬
    اكتب("عدد - ٢ = "، اثنانـعدد)‬
    اكتب("عدد + ١٩٩ = "، عدد + ١٩٩)
‫آخرلو عدد < ٢:‬
‫    اثنانـعدد = عدد + ٢‬
    اكتب("عدد + ٢ = "، اثنانـعدد)‬
‫آخر:‬
    اكتب("عدد < ٢")‬


حدد جور(اولا، ثاني):
    كل = اولا + ثاني
    إرجاع كل


اكتب("جور = "، جور(٢، ٣))


حدد قائمة(اولا، ثاني):
    كل = [اولا، ثاني]
    إرجاع كل


اكتب("قائمة = "، قائمة(٢، ٣))
